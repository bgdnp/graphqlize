    "generator"
  ],
  "author": "Bogdan Petkovic <bogdanpetkovic@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bgdnp/graphqlize/issues"
  },
  "homepage": "https://github.com/bgdnp/graphqlize#readme",
  "dependencies": {
    "graphql": "^14.5.8",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "apollo-server": "^2.9.12",
    "apollo-server-express": "^2.11.0",
    "express": "^4.17.1",
    "rimraf": "^3.0.2",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  }
