{
  "name": "@bgdn/graphqlize",
  "version": "1.2.3",
  "description": "Write your GraphQL schema in typescript",
  "main": "dist/index.js",
  "files": [
    "LICENSE",
    "README.md",
    "/dist",
    "!/dist/tsconfig.tsbuildinfo"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "build:dev": "rimraf dist && tsc --sourceMap",
    "dev": "ts-node dev",
